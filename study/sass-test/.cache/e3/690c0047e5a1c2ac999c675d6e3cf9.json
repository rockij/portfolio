ine":184,"column":2}},{"source":"main.scss","name":null,"original":{"line":206,"column":8},"generated":{"line":184,"column":14}},{"source":"main.scss","name":null,"original":{"line":207,"column":8},"generated":{"line":185,"column":2}},{"source":"main.scss","name":null,"original":{"line":207,"column":8},"generated":{"line":185,"column":18}},{"source":"main.scss","name":null,"original":{"line":216,"column":8},"generated":{"line":188,"column":0}},{"source":"main.scss","name":null,"original":{"line":217,"column":12},"generated":{"line":189,"column":2}},{"source":"main.scss","name":null,"original":{"line":217,"column":12},"generated":{"line":189,"column":12}},{"source":"main.scss","name":null,"original":{"line":216,"column":8},"generated":{"line":191,"column":0}},{"source":"main.scss","name":null,"original":{"line":217,"column":12},"generated":{"line":192,"column":2}},{"source":"main.scss","name":null,"original":{"line":217,"column":12},"generated":{"line":192,"column":13}},{"source":"main.scss","name":null,"original":{"line":216,"column":8},"generated":{"line":194,"column":0}},{"source":"main.scss","name":null,"original":{"line":217,"column":12},"generated":{"line":195,"column":2}},{"source":"main.scss","name":null,"original":{"line":217,"column":12},"generated":{"line":195,"column":13}},{"source":"main.scss","name":null,"original":{"line":216,"column":8},"generated":{"line":197,"column":0}},{"source":"main.scss","name":null,"original":{"line":217,"column":12},"generated":{"line":198,"column":2}},{"source":"main.scss","name":null,"original":{"line":217,"column":12},"generated":{"line":198,"column":13}},{"source":"main.scss","name":null,"original":{"line":234,"column":4},"generated":{"line":201,"column":0}},{"source":"main.scss","name":null,"original":{"line":235,"column":8},"generated":{"line":202,"column":2}},{"source":"main.scss","name":null,"original":{"line":235,"column":8},"generated":{"line":202,"column":14}},{"source":"main.scss","name":null,"original":{"line":236,"column":8},"generated":{"line":203,"column":2}},{"source":"main.scss","name":null,"original":{"line":236,"column":8},"generated":{"line":203,"column":58}},{"source":"main.scss","name":null,"original":{"line":234,"column":4},"generated":{"line":206,"column":0}},{"source":"main.scss","name":null,"original":{"line":235,"column":8},"generated":{"line":207,"column":2}},{"source":"main.scss","name":null,"original":{"line":235,"column":8},"generated":{"line":207,"column":14}},{"source":"main.scss","name":null,"original":{"line":236,"column":8},"generated":{"line":208,"column":2}},{"source":"main.scss","name":null,"original":{"line":236,"column":8},"generated":{"line":208,"column":58}},{"source":"main.scss","name":null,"original":{"line":234,"column":4},"generated":{"line":211,"column":0}},{"source":"main.scss","name":null,"original":{"line":235,"column":8},"generated":{"line":212,"column":2}},{"source":"main.scss","name":null,"original":{"line":235,"column":8},"generated":{"line":212,"column":14}},{"source":"main.scss","name":null,"original":{"line":236,"column":8},"generated":{"line":213,"column":2}},{"source":"main.scss","name":null,"original":{"line":236,"column":8},"generated":{"line":213,"column":58}}],"sources":{"_mixin.scss":"\r\n// 가변하는 인수 ...\r\n@mixin var($w, $h, $bg...){\r\n    width:$w;\r\n    height:$h;\r\n    background:$bg;\r\n}\r\n\r\n@mixin font(\r\n    $style: normal,\r\n    $weight: normal,\r\n    $size: 16px,\r\n    $family: sans-serif\r\n){\r\n    font: {\r\n        style: $style;\r\n        weight: $weight;\r\n        size: $size;\r\n        family: $family;\r\n    }\r\n}\r\n\r\n// @content\r\n@mixin icon ($url){\r\n    &::after{\r\n        content:$url;\r\n        @content;\r\n    }\r\n}\r\n \r\n/*\r\nif조건\r\nㄴunitless($w) : 단위가 있는지 없는지 체크\r\n*/\r\n@mixin positionCenter($w, $h, $p:absolute){\r\n    @if($p == absolute\r\n        or $p == fixed\r\n        or not $p == relative\r\n    ) {\r\n        width:if(unitless($w), #{$w}px, $w);\r\n        height:$h;\r\n        position:$p;\r\n        top:0;\r\n        bottom:0;\r\n        left:0;\r\n        right:0;\r\n        margin:auto;\r\n        background:red;\r\n    }\r\n}","main.scss":"@import 'mixin';\r\n\r\n// 문자보간 #{}을 이용해 코드의 어디든지 변수 값을 넣을 수 있다\r\n$family: unquote(\"Droid+sans\"); //sass의 내장함수\r\n@import url(\"http://fonts.googleapis.com/css?family=#{$family}\");\r\n\r\n.container{\r\n    $size: 100px;\r\n    .item{\r\n        width:$size;\r\n        height:$size;\r\n        background:tomato\r\n    }\r\n}\r\n\r\n// 중첩 밖에서 사용해야할 경우\r\n.list{\r\n    $w: 100px !global;\r\n    $h: 100px !global;\r\n    li{\r\n        width:$w;\r\n        height:$h;\r\n    }\r\n    @at-root .box2{\r\n        width:$w;\r\n        height:$h;\r\n        background:tomato;\r\n    }\r\n}\r\n\r\n// 동일한 네임 스페이스를 가지는 속성들\r\n.box{\r\n    font:{\r\n        weight: bold;\r\n        size: 10px;\r\n        font-family: Arial, \"Helvetica Neue\", Helvetica, sans-serif;\r\n    }\r\n    margin:{\r\n        top: 10px;\r\n        left: 20px;\r\n    }\r\n    padding:{\r\n        bottom:40px;\r\n        right:30px;\r\n    }\r\n}\r\n\r\n// 전역설정 !global(플래그) 변수의 유효범위를 설정\r\n$color: red;\r\n.box3{\r\n    width:$w;\r\n    height:$h;\r\n    $color: #111 !global;\r\n    background:$color;\r\n}\r\n.box3_1{\r\n    width:$w;\r\n    height:$h;\r\n    background:$color\r\n}\r\n.box3_2{\r\n    $color:blue;\r\n    width:$w;\r\n    height:$h;\r\n    background:$color\r\n}\r\n\r\n// 변수 초기값 설정 !default(플래그) 할당되지 않은 변수의 초기값 설정 할당되어 있는 변수가 있다면 변수가 기존 할당 값을 사용\r\n$color: red;\r\n.box4{\r\n    width:$w;\r\n    height:$h;\r\n    $color: #111 !default;\r\n    background:$color;\r\n}\r\n.box4_1{\r\n    $color: #111 !default;\r\n    width:$w;\r\n    height:$h;\r\n    background:$color\r\n}\r\n\r\n// color 연산\r\n.box5{\r\n    width:$w;\r\n    height:$h;\r\n    $color:rgba(0,0,0,.5);\r\n    color:opacify($color, .3); // 30% 더 불투명하게 / .5 + .3\r\n    background-color:transparentize($color, .2) // 20% 더 투명하게 / .5 - .2\r\n}\r\n\r\n/*\r\n논리연산 if\r\n    ㄴand 그리고\r\n    ㄴor 또는\r\n    ㄴnot 부정(분대)\r\n*/\r\n.box6{\r\n    $w:100px;\r\n    display:block;\r\n    @if ($w > 50px and $w > 90px){\r\n        width:400px;\r\n    }\r\n    @if not ($w < 50px){ // 조건의 반대\r\n        width:400px;\r\n    }\r\n}\r\n\r\n// 가변인수의 개념\r\n.box7{\r\n    @include var(\r\n        100px,\r\n        200px,\r\n        url(\"image/01.png\") no-repeat 10px 20px,\r\n        url(\"image/02.png\") no-repeat\r\n    );\r\n}\r\n.box8{\r\n    div{\r\n        // 매개변수 순서와 개수에 맞게 전달\r\n        $font-values: italic, bold, 16px, sans-serif;\r\n        @include font($font-values...);\r\n    }\r\n    span{\r\n        // 필요한 값만 키워드 인수로 변수에 담아 전달\r\n        $font-values: (style: italic, size: 22px); // map 데이터 형태\r\n        @include font($font-values...);\r\n    }\r\n    a{\r\n        // 필요한 값만 키워드 인수로 전달\r\n        @include font((weight:900, family: monospace)...);\r\n    }\r\n}\r\n\r\n// @content\r\n.box9{\r\n    @include icon(\"image/01.png\") {\r\n        display:block;\r\n        width:$w;\r\n        height:$h;\r\n    };\r\n}\r\n\r\n// @extend\r\n.box10{\r\n    @extend .box5;\r\n    background:darkblue;    \r\n}\r\n\r\n// 함수\r\n@function columns($number:1, $columns:12, $width:1200px){\r\n    @return $width * ($number / $columns);\r\n}\r\n.box11{\r\n    $width:980px;\r\n    li:nth-child(1){\r\n        width: columns();\r\n    }\r\n    li:nth-child(2){\r\n        width: columns(8);\r\n    }\r\n    li:nth-child(3){\r\n        width: columns(3);\r\n    }\r\n}\r\n\r\n// if함수\r\n.box12{\r\n    $w:550px;\r\n    width: if($w>300px, $w, null);\r\n    height:100px;\r\n    background:red\r\n}\r\n\r\n// if조건문(지시어)\r\n@function limitSize($size){\r\n    @if ($size >= 0 and $size <= 200px){\r\n        @return 200px;\r\n    }@else {\r\n        @return 800px;\r\n    }\r\n}\r\n.box13{\r\n    width: limitSize(180px);\r\n    height: limitSize(340px);\r\n    background:red;\r\n}\r\n.box14{\r\n    @include positionCenter(100px, 100px, fixed);\r\n}\r\n.box15{\r\n    @include positionCenter(100px, 100px, relative);\r\n}\r\n\r\n// for문\r\n@for $index from 1 through 3 {\r\n    .through li:nth-child(#{$index}){\r\n        width:20px * $index;\r\n        height:20px * $index;\r\n        background:red\r\n    }\r\n}\r\n@for $index from 2 to 5 {\r\n    .to li:nth-child(#{$index}){\r\n        width:20px * $index;\r\n        height:20px * $index;\r\n        background:blue\r\n    }\r\n}\r\n\r\n// each반복문\r\n$fruits: (apple, orange, banna, mango);\r\n.fruits{\r\n    @each $fruit in $fruits {\r\n        $index: index($fruits, $fruit);\r\n        li:nth-child(#{$index}){\r\n            left:50px * $index;\r\n            // background: url(\"image/#{$fruit}.png\");\r\n\r\n        }\r\n    }\r\n}\r\n// ease반복문 map\r\n$fruits-data: (\r\n    apple: korea,\r\n    orange: china,\r\n    banana: japan\r\n);\r\n@each $fruit, $country in $fruits-data{\r\n    // map-keys($fruits-data) => (apple, orange, banana)\r\n    // map-values($fruits-data) => (korea, china, japan)\r\n    $fruits-data-key-list: map-keys($fruits-data);\r\n    $index: index($fruits-data-key-list, $fruit);\r\n    .fruits-data li.#{$fruit}{\r\n        width:100px * $index;\r\n        background:url(\"image/#{$country}.png\");\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"e25c9aabd81a445abc7c3895fbad234e","cacheData":{"env":{}}}